# Configuration file for multipacting calculations

# Script control
version=0.9
parallel=4 # N processes (>1 parallel, ==1 serial)
log=False # Default False
debug=False # Default False
plot=False # Default True
verbose=True # Default True

# Simulation control
electrons_seed=100 # Number of initial electrons for each power, randomly distributed
electrons_source=1 # Options: 1 -> random surfaces (except excluded), 2 -> FN field effect
N_max_secondary_runs=62
delta_t=1e-12 # seconds, default 1e-10
RF_frequency=704.4e6 # Hz
N_cycles=500 # (default 20) Number of RF cycles per electron before forgetting it (t_max=N_cycles/RF_frequency)
#RF_power=range(50e3,1500e3,50e3) # Watts. Options: range (converted to np.arange), list [50,100,150] or single value
RF_power=4e6
simulation_type=3 # Options: 1-> power range, 2 -> single electron trajectory, 3 -> single electron multipacting
random_seed=2 # Options: -1 -> random seed, N>=0 -> random seed N. Default: -1. Multipacting example: 2.

# Model control
data_file=coaxial_103mm_704MHz_EB_field.txt
mesh_file=coaxial_103mm_704MHz.mphtxt.h5
#comsol_solid_domains=[1,3] # list of domains to be considered as solid
# Boundaries excluded. Several lines, accumulative. 
# Boolean selection: [-1|0|1 -> < == , 0|1|2 -> x,y,z , tolerance]
boundaries_excluded_boolean=(0,2,0.0,1e-2)
boundaries_excluded_boolean=(0,2,0.1,1e-2)

# Ejemplo avalancha: Power=4000000.0 W, initial #electrons: 1
# Run 1 electron starting from face=380, phase=3.6781046470434156, 
plot_title='coaxial_704_01.mpc'